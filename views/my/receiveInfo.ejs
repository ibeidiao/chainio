<!DOCTYPE HTML>
<html>
  <head>
    <%- include('../common/head.ejs') %>
    <title><%= title %>职业信用联盟</title>
    <% if (env !== 'dev') { %>
      <link rel="stylesheet" type="text/css" charset="utf-8" href="/dist/css/my/receiveInfo.css?v=<%= ts %>">
    <% } %>
  </head>
  <body>
    <div class="page-wrap">
      <%- include('../common/my/header.ejs', {cname: '杭州有才信息技术有限公司'}) %>
      <%- include('../common/my/leftNav.ejs', {page: 'receive'}) %>
      <div class="main-wrap">
        <ul class="bread-crumb"><li><a href="/my/receive">收到的背调</a></li><li class="s">/</li><li>详情</li></ul>
        <h2 class="main-title">订单信息</h2>
        <div class="form-action info-action">
          <div class="form-row">
            <label for="" class="text-lab">姓名</label>
            <p class="content"><%= name %></p>
          </div>
          <div class="form-row">
            <label for="" class="text-lab">部门</label>
            <p class="content"><%= dept %></p>
          </div>
          <div class="form-row">
            <label for="" class="text-lab">核实内容</label>
            <p class="content">工作履历核实</p>
          </div>
          <div class="form-row btn-form-row">
            <p class="content"><a href="/my/reply/<%= id %>" class="btn-main btn-reply">回复</a></p>
          </div>
        </div>
      </div>
      <%- include('../common/my/footer.ejs') %>
    </div>
    <script type="text/javascript" src="/dist/receiveInfo.js?v=<%= ts %>"></script>
  </body>
</html>